#include <iostream>
using namespace std;

int main() {
int a, n;
cout « "Введите числа a и n: ";
cin » a » n;

int b = 1;
while (b < n) {
if ((a * b) % n == 1) {
cout « "Обратный элемент к числу " « a « " в кольце вычетов по модулю " « n « " равен " « b;
return 0;
}
b++;
}

cout « "Обратный элемент не найден";
return 0;
}
